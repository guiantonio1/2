programa
{
	inclua biblioteca Matematica
--> m
	
	funcao inicio()
	{
        inteiro tempo = 5
        inteiro dias = 6
        inteiro semanas_por_ano = 52
        inteiro dias_anos
        inteiro leituras_horas

        dias_anos = dias * semanas_por_ano
        leituras_horas = (dias_anos * tempo) / 60


         escreva("Essa pessoa leu um total de ",leituras_horas," horas durante um ano!")
        
     
	}
}
