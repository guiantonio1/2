programa
{
	inclua biblioteca Matematica
 --> m
	
	funcao inicio()
	{
		real Terra   = 365.35 
		real Venus   = 0.61519726
		real Marte   = 1.8808158
		real Jupiter = 11.862615
		real Urano   = 84.016846
		real Netuno  = 164.79132
		real Sol1
		real Sol2 
		real Sol3
		real Sol4
		real Sol5

          Sol1 = Terra * Venus
		Sol2 = Terra * Marte
		Sol3 = Terra * Jupiter
		Sol4 = Terra * Urano 
		Sol5 = Terra * Netuno

	     escreva("Vênus demora cerca de " , m.arredondar(Sol1, 2)," dias terrestre para dar uma volta ao redor do sol!\n")
          escreva("Marte demora cerca de " , m.arredondar(Sol2, 2)," dias terrestre para dar uma volta ao redor do sol!\n")
          escreva("Júpiter demora cerca de " , m.arredondar(Sol3, 2)," dias terrestre para dar uma volta ao redor do sol!\n")
          escreva("Urano demora cerca de " , m.arredondar(Sol4, 2)," dias terrestre para dar uma volta ao redor do sol!\n")
          escreva("Netuno demora cerca de " , m.arredondar(Sol5, 2)," dias terrestre para dar uma volta ao redor do sol!\n")
        //A 
        //2
	}
 
}



programa
{
	
	funcao inicio()
